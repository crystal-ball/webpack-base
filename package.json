{
  "name": "@crystal-ball/webpack-base",
  "version": "0.0.0-semantic",
  "description": "Customizable webpack base configuration generator",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/webpack-base.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "webpack",
    "configuration",
    "customizable"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=8.11"
  },
  "scripts": {
    "commit": "git cz",
    "container": "docker-compose run --service-ports --rm app",
    "container:test": "docker-compose run --rm webpack",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest src",
    "test:ci": "docker-compose -f docker-compose.test.yml up --exit-code-from cypress",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "format": "prettier --write \"{scripts,src,test-app}/**/*.{js,scss,md}\"",
    "lint": "NODE_ENV=test eslint scripts src test-app",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "@babel/core": "~7.2.2",
    "@babel/plugin-proposal-class-properties": "~7.3.0",
    "@babel/plugin-transform-react-jsx-source": "^7.2.0",
    "@babel/plugin-transform-runtime": "~7.2.0",
    "@babel/preset-env": "~7.3.1",
    "@babel/preset-react": "~7.0.0",
    "@svgr/webpack": "~4.1.0",
    "autoprefixer": "~9.4.6",
    "babel-loader": "~8.0.5",
    "babel-plugin-styled-components": "~1.10.0",
    "chalk": "~2.4.2",
    "clean-webpack-plugin": "~1.0.1",
    "compression-webpack-plugin": "~2.0.0",
    "copy-webpack-plugin": "~4.6.0",
    "css-loader": "~2.1.0",
    "duplicate-package-checker-webpack-plugin": "~3.0.0",
    "eslint-import-resolver-webpack": "~0.11.0",
    "eslint-loader": "~2.1.1",
    "file-loader": "~3.0.1",
    "friendly-errors-webpack-plugin": "~1.7.0",
    "html-webpack-plugin": "~3.2.0",
    "mini-css-extract-plugin": "~0.5.0",
    "node-sass": "~4.11.0",
    "postcss-custom-properties": "~8.0.9",
    "postcss-loader": "~3.0.0",
    "progress-bar-webpack-plugin": "~1.12.0",
    "raw-loader": "~1.0.0",
    "sass-loader": "~7.1.0",
    "style-loader": "~0.23.1",
    "svg-symbol-sprite-loader": "~3.1.0",
    "webpack": "~4.29.0",
    "webpack-cli": "~3.2.1",
    "webpack-dev-server": "~3.1.14",
    "webpack-merge": "~4.2.1",
    "webpack-monitor": "~1.0.14",
    "snyk": "^1.189.0"
  },
  "devDependencies": {
    "commitizen": "^3.0.5",
    "cypress": "~3.1.4",
    "cz-customizable": "^5.3.0",
    "eslint-config-eloquence": "^10.1.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "prettier": "^1.16.1",
    "semantic-release": "^15.13.3"
  },
  "husky": {
    "prepush": "npm run test"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "snyk": true
}
