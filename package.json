{
  "name": "@inspirescript/webpack-configs",
  "version": "2.21.0",
  "description": "InspireScript base wepback configs",
  "main": "index.js",
  "bin": {
    "generate-inspirescript-project": "./scripts/generate-new-project.js"
  },
  "scripts": {
    "commit": "git cz",
    "test": "npm run lint",
    "prepush": "npm run test",
    "documentation": "documentation build src/** -f html -o docs --require-extension mjs --parse-extension mjs",
    "format": "prettier --write \"{src,project-template}/**/*.{mjs,js,jsx}\"",
    "lint": "NODE_ENV=test eslint src/*"
  },
  "keywords": [
    "InspireScript",
    "webpack",
    "configs"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/inspirescript/webpack-configs.git"
  },
  "dependencies": {
    "@std/esm": "^0.16.0",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-stateless-component-name": "^1.1.2",
    "babel-preset-env": "~1.6.1",
    "babel-preset-react": "~6.24.1",
    "clean-webpack-plugin": "~0.1.17",
    "copy-webpack-plugin": "^4.2.1",
    "css-loader": "^0.28.7",
    "duplicate-package-checker-webpack-plugin": "~1.2.6",
    "eslint-loader": "~1.9.0",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "inline-chunk-manifest-html-webpack-plugin": "^2.0.0",
    "node-sass": "~4.7.2",
    "nyan-progress-webpack-plugin": "^1.2.0",
    "postcss-loader": "^2.0.8",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "~6.0.6",
    "shelljs": "^0.7.8",
    "style-loader": "~0.19.0",
    "svg-symbol-sprite-loader": "^0.1.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "~4.1.0",
    "webpack-monitor": "^1.0.13"
  },
  "devDependencies": {
    "@crystal-ball/eslint-config-eloquence": "^6.2.0",
    "commitizen": "^2.9.6",
    "cz-customizable": "^5.2.0",
    "documentation": "^5.3.3",
    "flow-bin": "^0.61.0",
    "husky": "^0.14.3",
    "prettier": "^1.9.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
