{
  "name": "@inspirescript/webpack-configs",
  "version": "2.20.0",
  "description": "InspireScript base wepback configs",
  "main": "index.js",
  "bin": {
    "generate-inspirescript-project": "./scripts/generate-new-project.js"
  },
  "scripts": {
    "test": "npm run eslint",
    "prepush": "npm run test",
    "postpublish": "npm run clean",
    "clean": "rm -rf build __mocks__ flow-typed monitor public src",
    "documentation":
      "./node_modules/.bin/documentation build lib/** -f html -o docs --require-extension mjs --parse-extension mjs",
    "test:start": "NODE_ENV=development webpack-dev-server --env=development",
    "test:build": "NODE_ENV=production webpack --env=production",
    "test:monitor":
      "LAUNCH_MONITOR=true NODE_ENV=production webpack --env=production",
    "test:test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier --write lib/*.mjs",
    "eslint": "NODE_ENV=test eslint lib/*"
  },
  "keywords": ["InspireScript", "webpack", "configs"],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/inspirescript/webpack-configs.git"
  },
  "peerDependencies": {
    "babel-eslint": "^8.0.0",
    "eslint": "^4.10.0"
  },
  "dependencies": {
    "@std/esm": "^0.16.0",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-stateless-component-name": "^1.1.2",
    "babel-preset-env": "~1.6.1",
    "babel-preset-react": "~6.24.1",
    "clean-webpack-plugin": "~0.1.17",
    "copy-webpack-plugin": "^4.2.1",
    "css-loader": "^0.28.7",
    "duplicate-package-checker-webpack-plugin": "~1.2.6",
    "eslint-loader": "~1.9.0",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "~4.7.2",
    "nyan-progress-webpack-plugin": "^1.2.0",
    "postcss-loader": "^2.0.8",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "~6.0.6",
    "shelljs": "^0.7.8",
    "style-loader": "~0.19.0",
    "svg-sprite-loader": "^3.4.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "~4.1.0",
    "webpack-monitor": "^1.0.13"
  },
  "devDependencies": {
    "@crystal-ball/eslint-config-eloquence": "^6.1.1",
    "documentation": "^5.3.3",
    "flow-bin": "^0.59.0",
    "husky": "^0.14.3",
    "prettier": "^1.8.2"
  }
}
