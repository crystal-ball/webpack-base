{
  "name": "@inspirescript/webpack-configs",
  "version": "3.0.0",
  "description": "InspireScript base wepback configs",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/inspirescript/webpack-configs.git"
  },
  "keywords": [
    "InspireScript",
    "webpack",
    "configuration"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=7.9"
  },
  "bin": {
    "generate-inspirescript-project": "./scripts/generate-new-project.js"
  },
  "scripts": {
    "commit": "git cz",
    "container": "docker-compose run --service-ports --rm app",
    "container:test": "docker-compose run --rm webpack",
    "test": "npm run lint && npm run test:unit",
    "prepush": "npm run test",
    "test:unit": "jest src",
    "test:acceptance": "jest test",
    "test:ci-acceptance": "docker-compose -f docker-compose.test.yml up --exit-code-from puppeteer",
    "format": "prettier --write \"{guides,scripts,src,test-app}/**/*.{js,md}\"",
    "lint": "NODE_ENV=test eslint src/**/*.js test-app/**/*.js"
  },
  "dependencies": {
    "@inspirescript/magic-markdown-loader": "~1.0.0",
    "autoprefixer": "~8.5.0",
    "babel-core": "~6.26.3",
    "babel-loader": "~7.1.4",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "~1.7.0",
    "babel-preset-react": "~6.24.1",
    "chalk": "~2.4.1",
    "clean-webpack-plugin": "~0.1.19",
    "copy-webpack-plugin": "~4.5.1",
    "css-loader": "~0.28.11",
    "directory-named-webpack-plugin": "^4.0.0",
    "duplicate-package-checker-webpack-plugin": "~3.0.0",
    "eslint-import-resolver-webpack": "~0.10.0",
    "eslint-loader": "~2.0.0",
    "file-loader": "~1.1.11",
    "friendly-errors-webpack-plugin": "~1.7.0",
    "html-webpack-plugin": "~3.2.0",
    "mini-css-extract-plugin": "~0.4.0",
    "node-sass": "~4.9.0",
    "postcss-loader": "~2.1.5",
    "progress-bar-webpack-plugin": "~1.11.0",
    "raw-loader": "~0.5.1",
    "sass-loader": "~7.0.1",
    "shelljs": "~0.8.2",
    "style-loader": "~0.21.0",
    "svg-symbol-sprite-loader": "~1.0.0",
    "webpack": "~4.8.3",
    "webpack-cli": "~2.1.4",
    "webpack-dev-server": "~3.1.4",
    "webpack-merge": "~4.1.2",
    "webpack-monitor": "~1.0.14"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-customizable": "^5.2.0",
    "eslint-config-eloquence": "^7.4.0",
    "flow-bin": "^0.73.0",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "prettier": "^1.12.1",
    "puppeteer": "^1.4.0",
    "semantic-release": "^15.5.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
