{
  "name": "@crystal-ball/webpack-base",
  "version": "1.0.1",
  "description": "Customizable webpack base configuration generator",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/webpack-base.git"
  },
  "keywords": [
    "webpack",
    "configuration",
    "customizable"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=7.9"
  },
  "bin": {
    "ðŸ”®": "./scripts/initialize-project.js",
    "webpack-base": "./scripts/initialize-project.js"
  },
  "scripts": {
    "commit": "git cz",
    "container": "docker-compose run --service-ports --rm app",
    "container:test": "docker-compose run --rm webpack",
    "test": "npm run lint && npm run test:unit",
    "prepush": "npm run test",
    "test:unit": "jest src",
    "test:acceptance": "jest test",
    "test:ci-acceptance": "docker-compose -f docker-compose.test.yml up --exit-code-from puppeteer",
    "format": "prettier --write \"{scripts,src,test-app}/**/*.{js,scss,md}\"",
    "lint": "NODE_ENV=test eslint scripts src test-app"
  },
  "dependencies": {
    "autoprefixer": "~8.6.2",
    "babel-core": "~6.26.3",
    "babel-loader": "~7.1.4",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "~1.7.0",
    "babel-preset-react": "~6.24.1",
    "chalk": "~2.4.1",
    "clean-webpack-plugin": "~0.1.19",
    "copy-webpack-plugin": "~4.5.1",
    "css-loader": "~0.28.11",
    "duplicate-package-checker-webpack-plugin": "~3.0.0",
    "eslint-import-resolver-webpack": "~0.10.0",
    "eslint-loader": "~2.0.0",
    "file-loader": "~1.1.11",
    "friendly-errors-webpack-plugin": "~1.7.0",
    "html-webpack-plugin": "~3.2.0",
    "mini-css-extract-plugin": "~0.4.0",
    "node-sass": "~4.9.0",
    "postcss-loader": "~2.1.5",
    "progress-bar-webpack-plugin": "~1.11.0",
    "raw-loader": "~0.5.1",
    "sass-loader": "~7.0.3",
    "shelljs": "~0.8.2",
    "style-loader": "~0.21.0",
    "svg-symbol-sprite-loader": "~2.1.0",
    "webpack": "~4.12.0",
    "webpack-cli": "~3.0.8",
    "webpack-merge": "~4.1.3",
    "webpack-monitor": "~1.0.14",
    "webpack-serve": "~1.0.4",
    "webpack-serve-waitpage": "~1.0.0"
  },
  "devDependencies": {
    "commitizen": "^2.10.1",
    "cz-customizable": "^5.2.0",
    "eslint-config-eloquence": "^7.4.0",
    "flow-bin": "^0.74.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "prettier": "^1.13.5",
    "puppeteer": "^1.5.0",
    "semantic-release": "^15.5.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
