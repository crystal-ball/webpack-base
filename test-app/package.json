{
  "name": "test-app",
  "version": "0.0.0",
  "description": "Package test application",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --mode=development",
    "start:docker": "NODE_ENV=development webpack-dev-server --host=0.0.0.0 --mode=development",
    "build": "NODE_ENV=production webpack --mode=production",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest src",
    "format": "prettier --write \"src/**/*.{js,md,scss}\"",
    "lint": "NODE_ENV=test eslint src"
  },
  "dependencies": {
    "@babel/runtime": "~7.3.1",
    "classnames": "~2.2.6",
    "componentry": "~3.0.0-alpha.12",
    "nanoid": "~2.0.1",
    "prop-types": "^15.6.2",
    "react": "~16.7.0",
    "react-dom": "~16.7.0",
    "react-redux": "~6.0.0",
    "react-router-dom": "~4.3.1",
    "redux": "~4.0.1",
    "redux-saga": "~1.0.0",
    "styled-components": "~4.1.3",
    "svg-symbol-sprite-loader": "~3.1.0"
  },
  "devDependencies": {
    "@crystal-ball/prettier-base": "1.2.0",
    "eslint-config-eloquence": "12.0.0",
    "jest": "24.8.0"
  },
  "prettier": "@crystal-ball/prettier-base"
}
