{
  "name": "test-app",
  "version": "0.0.0",
  "description": "InspireScript webpack configs test application",
  "scripts": {
    "build": "NODE_ENV=production webpack --env=production",
    "stats": "LAUNCH_MONITOR=true NODE_ENV=production webpack --env=production",
    "start": "webpack-serve --host=0.0.0.0",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest src",
    "flow": "flow",
    "format": "prettier --write \"src/**/*.{js,md}\"",
    "lint": "NODE_ENV=test eslint src"
  },
  "keywords": [
    "inspirescript"
  ],
  "contributors": [],
  "license": "ISC",
  "dependencies": {
    "classnames": "~2.2.6",
    "componentry": "~2.3.1",
    "nanoid": "~1.0.3",
    "prop-types": "^15.6.1",
    "react": "~16.4.0",
    "react-dom": "~16.4.0",
    "react-hot-loader": "~4.3.1",
    "react-redux": "~5.0.7",
    "react-router-dom": "~4.3.1",
    "redux": "~4.0.0",
    "redux-saga": "0.16.0",
    "svg-symbol-sprite-loader": "~1.1.0"
  },
  "devDependencies": {
    "@inspirescript/webpack-configs": "latest",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint-config-eloquence": "^7.4.0",
    "flow-bin": "^0.74.0",
    "prettier": "^1.13.5"
  }
}
