{
  "name": "test-app",
  "version": "0.0.0",
  "description": "Package test application",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --mode=development",
    "start:docker": "NODE_ENV=development webpack-dev-server --host=0.0.0.0 --mode=development",
    "build": "NODE_ENV=production webpack --mode=production",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest src",
    "format": "prettier --write \"src/**/*.{js,md,scss}\"",
    "lint": "NODE_ENV=test eslint src"
  },
  "dependencies": {
    "@babel/runtime": "7.5.5",
    "@babel/runtime-corejs3": "7.5.5",
    "classnames": "2.2.6",
    "componentry": "3.0.0-alpha.12",
    "nanoid": "2.1.0",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4",
    "redux-saga": "1.0.5",
    "styled-components": "4.3.2",
    "svg-symbol-sprite-loader": "3.2.0"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "1.10.6",
    "@crystal-ball/prettier-base": "1.4.0",
    "eslint-config-eloquence": "12.4.0",
    "jest": "24.9.0"
  },
  "prettier": "@crystal-ball/prettier-base"
}
