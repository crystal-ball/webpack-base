{
  "name": "test-app",
  "version": "0.0.0",
  "description": "Package test application",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --mode=development",
    "start:docker": "NODE_ENV=development webpack-dev-server --host=0.0.0.0 --mode=development",
    "build": "NODE_ENV=production webpack --mode=production",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest src",
    "format": "prettier --write \"src/**/*.{js,md,scss}\"",
    "lint": "NODE_ENV=test eslint src"
  },
  "dependencies": {
    "@babel/runtime": "~7.1.2",
    "classnames": "~2.2.6",
    "componentry": "~3.0.0-alpha.5",
    "nanoid": "~1.2.6",
    "prop-types": "^15.6.2",
    "react": "~16.5.2",
    "react-dom": "~16.5.2",
    "react-hot-loader": "~4.3.11",
    "react-redux": "~5.0.7",
    "react-router-dom": "~4.3.1",
    "redux": "~4.0.0",
    "redux-saga": "~0.16.0",
    "styled-components": "~3.4.9",
    "svg-symbol-sprite-loader": "~2.1.0"
  },
  "devDependencies": {
    "eslint-config-eloquence": "^9.5.0",
    "prettier": "^1.14.3"
  }
}
