{
  "name": "test-app",
  "version": "0.0.0",
  "description": "Package test application",
  "scripts": {
    "start": "webpack-dev-server --mode=development",
    "start:docker": "webpack-dev-server --host=0.0.0.0 --mode=development",
    "build": "webpack --mode=production",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest src",
    "flow": "flow",
    "format": "prettier --write \"src/**/*.{js,md,scss}\"",
    "lint": "NODE_ENV=test eslint src"
  },
  "dependencies": {
    "classnames": "~2.2.6",
    "componentry": "~2.3.1",
    "nanoid": "~1.2.4",
    "prop-types": "^15.6.2",
    "react": "~16.5.2",
    "react-dom": "~16.5.2",
    "react-hot-loader": "~4.3.11",
    "react-redux": "~5.0.7",
    "react-router-dom": "~4.3.1",
    "redux": "~4.0.0",
    "redux-saga": "0.16.0",
    "svg-symbol-sprite-loader": "~2.1.0"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint-config-eloquence": "^9.3.2",
    "flow-bin": "^0.81.0",
    "prettier": "^1.14.3"
  }
}
