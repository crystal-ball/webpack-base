{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": { "browsers": [">0.25%", "not ie 11", "not op_mini all"] }
      }
    ],
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-runtime" // Needed for generators and babel-helpers
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties",
        "transform-runtime" // Needed for generators and babel-helpers
      ]
    }
  }
}
